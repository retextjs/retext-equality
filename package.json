{
  "name": "retext-equality",
  "version": "5.7.0",
  "description": "retext plugin to warn about possible insensitive, inconsiderate language",
  "license": "MIT",
  "keywords": [
    "unified",
    "retext",
    "retext-plugin",
    "plugin",
    "equal",
    "race",
    "religion",
    "sex",
    "orientation",
    "identity",
    "gender",
    "sensitive",
    "insensitive",
    "neutral",
    "writing",
    "language",
    "english"
  ],
  "repository": "retextjs/retext-equality",
  "bugs": "https://github.com/retextjs/retext-equality/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Shinnosuke Watanabe <snnskwtnb@gmail.com>",
    "Elliott Hauser <eah13@mac.com>",
    "Ryan Tucker <git@ryantucker.us>",
    "David Simons <david.simons@softwire.com>",
    "rugk <rugk@posteo.de>",
    "Eli Feasley <eliana@khanacademy.org>",
    "Eli Sadoff <esadoff@ardiangroup.com>",
    "Flip Stewart <flipstewart@me.com>",
    "Catherine Etter <cetter.catherine@gmail.com>",
    "Conlin Durbin <conlindurbin@protonmail.com>",
    "Jen Weber <j@jenweber.me>",
    "Matsuko <info@matsuko.ca>",
    "Saksham Gupta <saksham.gupta2016@vitstudent.ac.in>",
    "Aaron Miller <me@aaron-miller.me>",
    "Alicia Gansley <aegansley@gmail.com>",
    "Anna K <anna.k.kautto@gmail.com>",
    "Bryce Kahle <bryce@brycekahle.com>",
    "Ben Hall <github@benhall.io>"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "files": [
    "lib/",
    "en.js",
    "index.js"
  ],
  "dependencies": {
    "nlcst-normalize": "^2.0.0",
    "nlcst-search": "^2.0.0",
    "nlcst-to-string": "^2.0.0",
    "quotation": "^1.0.0",
    "unist-util-is": "^4.0.0",
    "unist-util-visit": "^2.0.0"
  },
  "devDependencies": {
    "array-duplicated": "^2.0.0",
    "array-unique": "^0.3.0",
    "bail": "^1.0.0",
    "browserify": "^17.0.0",
    "c8": "^7.0.0",
    "chalk": "^4.0.0",
    "is-hidden": "^1.0.0",
    "js-yaml": "^4.0.0",
    "mdast-util-heading-range": "^2.0.0",
    "not": "^0.1.0",
    "prettier": "^2.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "retext": "^7.0.0",
    "tape": "^5.0.0",
    "tinyify": "^3.0.0",
    "unist-builder": "^2.0.0",
    "vfile-sort": "^2.0.0",
    "xo": "^0.39.0"
  },
  "scripts": {
    "generate": "node script/generate.js",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test.js",
    "test-coverage": "c8 --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter lcov node --conditions development test.js",
    "test": "npm run generate && npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "max-depth": "off",
      "no-var": "off",
      "prefer-arrow-callback": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/prefer-includes": "off",
      "guard-for-in": "off",
      "max-params": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "gfm",
        {
          "tablePipeAlign": false
        }
      ],
      [
        "remark-lint-table-pipe-alignment",
        false
      ],
      "./script/build-table"
    ]
  }
}
